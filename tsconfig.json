{
	"compilerOptions": {
		"target": "es2022",
		"module": "ESNext",
		"baseUrl": ".",
		"rootDir": ".",
		"allowJs": true,
		"paths": {
			"@taskchain/*": ["./src/*"]
		},
		"noEmit": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": false,
		"skipLibCheck": true
	}
}
